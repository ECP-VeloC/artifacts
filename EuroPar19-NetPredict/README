  author = {Shu-Mei Tseng and Bogdan Nicolae and George Bosilca and Emmanuel Jeannot and Aparna Chandramowlishwaran and Franck Cappello},
  title = {Towards Portable Online Prediction of Network Utilization using MPI-level Monitoring},
